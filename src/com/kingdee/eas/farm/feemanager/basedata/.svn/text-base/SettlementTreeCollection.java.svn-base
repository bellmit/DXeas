        return removeObject(item);
    }
    public SettlementTreeInfo get(int index)
    {
        return(SettlementTreeInfo)getObject(index);
    }
    public SettlementTreeInfo get(Object key)
    {
        return(SettlementTreeInfo)getObject(key);
    }
    public void set(int index, SettlementTreeInfo item)
    {
        setObject(index, item);
    }
    public boolean contains(SettlementTreeInfo item)
