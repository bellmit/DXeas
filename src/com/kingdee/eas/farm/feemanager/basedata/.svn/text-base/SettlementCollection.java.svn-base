package com.kingdee.eas.farm.feemanager.basedata;

import com.kingdee.bos.dao.AbstractObjectCollection;
import com.kingdee.bos.dao.IObjectPK;

public class SettlementCollection extends AbstractObjectCollection 
{
    public SettlementCollection()
    {
        super(SettlementInfo.class);
    }
    public boolean add(SettlementInfo item)
    {
        return addObject(item);
    }
    public boolean addCollection(SettlementCollection item)
    {
        return addObjectCollection(item);
    }
    public boolean remove(SettlementInfo item)
    {
        return removeObject(item);
    }
    public SettlementInfo get(int index)
    {
        return(SettlementInfo)getObject(index);
    }
    public SettlementInfo get(Object key)
    {
        return(SettlementInfo)getObject(key);
    }
    public void set(int index, SettlementInfo item)
    {
        setObject(index, item);
    }
    public boolean contains(SettlementInfo item)
    {
        return containsObject(item);
    }
    public boolean contains(Object key)
    {
        return containsKey(key);
    }
    public int indexOf(SettlementInfo item)
    {
        return super.indexOf(item);
    }
}