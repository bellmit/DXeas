package com.kingdee.eas.farm.feemanager.basedata;

import com.kingdee.bos.dao.AbstractObjectCollection;
import com.kingdee.bos.dao.IObjectPK;

public class DriverCollection extends AbstractObjectCollection 
{
    public DriverCollection()
    {
        super(DriverInfo.class);
    }
    public boolean add(DriverInfo item)
    {
        return addObject(item);
    }
    public boolean addCollection(DriverCollection item)
    {
        return addObjectCollection(item);
    }
    public boolean remove(DriverInfo item)
    {
        return removeObject(item);
    }
    public DriverInfo get(int index)
    {
        return(DriverInfo)getObject(index);
    }
    public DriverInfo get(Object key)
    {
        return(DriverInfo)getObject(key);
    }
    public void set(int index, DriverInfo item)
    {
        setObject(index, item);
    }
    public boolean contains(DriverInfo item)
    {
        return containsObject(item);
    }
    public boolean contains(Object key)
    {
        return containsKey(key);
    }
    public int indexOf(DriverInfo item)
    {
        return super.indexOf(item);
    }
}