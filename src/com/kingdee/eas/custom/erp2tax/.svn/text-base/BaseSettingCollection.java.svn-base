package com.kingdee.eas.custom.erp2tax;

import com.kingdee.bos.dao.AbstractObjectCollection;
import com.kingdee.bos.dao.IObjectPK;

public class BaseSettingCollection extends AbstractObjectCollection 
{
    public BaseSettingCollection()
    {
        super(BaseSettingInfo.class);
    }
    public boolean add(BaseSettingInfo item)
    {
        return addObject(item);
    }
    public boolean addCollection(BaseSettingCollection item)
    {
        return addObjectCollection(item);
    }
    public boolean remove(BaseSettingInfo item)
    {
        return removeObject(item);
    }
    public BaseSettingInfo get(int index)
    {
        return(BaseSettingInfo)getObject(index);
    }
    public BaseSettingInfo get(Object key)
    {
        return(BaseSettingInfo)getObject(key);
    }
    public void set(int index, BaseSettingInfo item)
    {
        setObject(index, item);
    }
    public boolean contains(BaseSettingInfo item)
    {
        return containsObject(item);
    }
    public boolean contains(Object key)
    {
        return containsKey(key);
    }
    public int indexOf(BaseSettingInfo item)
    {
        return super.indexOf(item);
    }
}